{
	"manifest_version": 2,
	"name": "Pegmatite",
	"version": "1.1.0",
	"description": "Shows graphs and diagrams rendered by PlantUML.",
	"permissions": [
		"storage"
	],
	"options_page": "options.html",
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"content_scripts": [
		{
			"js": [
				"rawdeflate.js",
				"content-script.js"
			],
			"matches": [
				"https://github.com/*",
				"https://gist.github.com/*"
			]
		}
	]
}
